<h1>This is all the Cryptids</h1>
<div>
	<% myCryptids.forEach((cryptid, index) =>{%>
		<div>
			<p><%=cryptid.name%></p> 
			<form method="POST" action="/cryptids/<%= index %>?_method=DELETE">
				<input type="submit" value="Delete it">
			</form>
			<form method="GET" action="/cryptids/<%= index %>/edit">
				<input type="submit" value="Edit Data">
			</form>
			<img src=<%=cryptid.img_url%> >
		</div>
	<% }) %>
</div>